<IfModule mod_ssl.c>
<VirtualHost *:443>
        ServerName community.unix.com

        ServerAdmin webmaster@localhost

        SSLProxyEngine on
  	RewriteEngine On

        RewriteCond %{HTTP_USER_AGENT}  (Bytespider|Yandex|Wget|seocompany|CCBot|Cincraw) [NC]
        RewriteRule . - [R=403,L]

  	ProxyPreserveHost On
  	ProxyRequests Off

        RequestHeader set X-Forwarded-Proto "https"
 	RequestHeader set X-Real-IP expr=%{REMOTE_ADDR}

        ProxyPass / unix:/var/disco/shared/socket2/nginx.http.sock|http://community.unix.com/
        ProxyPassReverse  / unix:/var/disco/shared/socket2/nginx.http.sock|http://community.unix.com/

        #LogLevel info ssl:warn

        ErrorLog ${APACHE_LOG_DIR}/community_errors_ssl.log
        #CustomLog ${APACHE_LOG_DIR}/community_ssl.log combined

       SSLCertificateFile /etc/letsencrypt/live/community.unix.com/fullchain.pem
       SSLCertificateKeyFile /etc/letsencrypt/live/community.unix.com/privkey.pem
       Include /etc/letsencrypt/options-ssl-apache.conf
</VirtualHost>
</IfModule>
